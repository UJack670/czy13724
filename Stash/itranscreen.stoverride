name: |-
  itranscreen
desc: |-
  显示pro3866即成功，请关闭其他语言类脚本。下载链接请访问本js链接

http:

  mitm:
    - "api.revenuecat.com"

  script:  
  - match: ^https?:\/\/api\.revenuecat\.com\/v1\/(subscribers\/[^\/]+$|receipts$)
    name: itranscreen_3
    type: request
    require-body: true
    max-size: -1
    timeout: 60


script-providers:
  "itranscreen_3":
    url: https://raw.githubusercontent.com/czy13724/Quantumult-X/main/scripts/itranscreen.js
    interval: 86400

